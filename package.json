{
  "name": "@sweatco/styled",
  "version": "0.2.0",
  "types": "./lib/typescript/module/src/index.d.ts",
  "source": "./src/index.ts",
  "main": "./lib/commonjs/index.js",
  "module": "./lib/module/index.js",
  "author": "Ivan Kuchaev <kuchaev.iy@gmail.com> (https://github.com/Augustach)",
  "license": "MIT",
  "scripts": {
    "test": "jest",
    "typecheck": "tsc",
    "lint": "eslint \"**/*.{js,ts,tsx}\"",
    "prepare": "bob build",
    "release": "release-it"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/sweatco/styled-native-components.git"
  },
  "keywords": [
    "react-native",
    "styled-components",
    "css-in-js",
    "css-to-react-native"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./lib/typescript/module/src/index.d.ts",
        "default": "./lib/module/index.js"
      },
      "require": {
        "types": "./lib/typescript/commonjs/src/index.d.ts",
        "default": "./lib/commonjs/index.js"
      }
    },
    "./native": {
      "import": {
        "types": "./lib/typescript/module/src/index.d.ts",
        "default": "./lib/module/index.js"
      },
      "require": {
        "types": "./lib/typescript/commonjs/src/index.d.ts",
        "default": "./lib/commonjs/index.js"
      }
    },
    "./babel-plugin": {
      "import": "./babel-plugin/index.js",
      "require": "./babel-plugin/index.js"
    }
  },
  "files": [
    "src",
    "lib",
    "native",
    "babel-plugin",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__",
    "!**/.*"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "workspaces": [
    "example"
  ],
  "dependencies": {
    "css-to-react-native": "^3.2.0",
    "postcss": "^8.4.38"
  },
  "devDependencies": {
    "@babel/cli": "^7.28.0",
    "@babel/types": "^7.28.0",
    "@commitlint/config-conventional": "^19.8.1",
    "@evilmartians/lefthook": "^1.12.2",
    "@react-native/babel-preset": "0.80.2",
    "@react-native/eslint-config": "^0.80.2",
    "@release-it/conventional-changelog": "^10.0.1",
    "@tsconfig/react-native": "^3.0.6",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.2.1",
    "@types/react": "^19.1.9",
    "commitlint": "^19.8.1",
    "eslint": "8.51.0",
    "eslint-plugin-ft-flow": "^3.0.11",
    "jest": "^30.0.5",
    "prettier": "^3.6.2",
    "react": "^19.1.1",
    "react-native": "^0.80.2",
    "react-native-builder-bob": "^0.40.13",
    "release-it": "^19.0.4",
    "typescript": "^5.9.2"
  },
  "peerDependencies": {
    "react": ">=18.2.0",
    "react-native": ">=0.73.0"
  },
  "resolutions": {
    "braces": "^3.0.3"
  },
  "packageManager": "yarn@4.1.1",
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "release-it": {
    "git": {
      "commitMessage": "chore: release ${version}",
      "tagName": "v${version}"
    },
    "npm": {
      "publish": true
    },
    "github": {
      "release": true
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": "angular"
      }
    }
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      [
        "commonjs",
        {
          "esm": true
        }
      ],
      [
        "module",
        {
          "esm": true
        }
      ],
      [
        "typescript",
        {
          "project": "tsconfig.build.json",
          "esm": true
        }
      ]
    ]
  }
}
